        <q1:SecurityOptions>
          <q1:KeyName>MACHINE\Software\Microsoft\Windows\CurrentVersion\Policies\System\Kerberos\Parameters\SupportedEncryptionTypes</q1:KeyName>
          <q1:SettingNumber>2147483640</q1:SettingNumber>
          <q1:Display>
            <q1:Name>Network security: Configure encryption types allowed for Kerberos</q1:Name>
            <q1:Units />
            <q1:DisplayFields>
              <q1:Field>
                <q1:Name>DES_CBC_CRC</q1:Name>
